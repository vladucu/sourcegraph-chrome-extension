{ 
	"manifest_version": 2,
	"name": "Sourcegraph chrome extension",
	"version": "0",
	"description": "Enhance code browsing on GitHub: semantic code search, instant documentaton tooltips, and jump-to-definition links for GitHub code.",
	"icons": {"128": "/assets/sg-sourcegraph-mark.png"},

	"content_scripts": [
		{
		"matches": ["https://github.com/*"],
		"css": ["sgsearch.css", "github.css", "defn_popover.css"],
		"js": ["jquery-1.12.0.min.js","sgsearch.js" ,"github.js", "defn-popover.js"]
		
		}],
	"web_accessible_resources": ["sgsearch.css", "github.css", "defn_popover.css", "/assets/src4.png", "/assets/search.png"],
	"background": {
		"scripts": ["background_page.js"]

	},
	"permissions": [
		"tabs",
		"activeTab",
		"https://sourcegraph.com/*",
		"http://sourcegraph.com/*",
		"http://*.sourcegraph.com/*",
		"https://*.sourcegraph.com/*",
		"storage"
		]
}